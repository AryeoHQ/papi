{
  "title": "Pet",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "format": "int64",
      "example": 10,
      "nullable": false,
      "x-nullable": false
    },
    "name": {
      "type": "string",
      "example": "doggie",
      "nullable": false,
      "x-nullable": false
    },
    "category": {
      "$ref": "./Category.json",
      "nullable": true,
      "x-nullable": true
    },
    "photoUrls": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "nullable": false,
      "x-nullable": false
    },
    "tags": {
      "type": "array",
      "items": {
        "$ref": "./Tag.json"
      },
      "nullable": true,
      "x-nullable": true
    },
    "status": {
      "type": "string",
      "description": "pet status in the store",
      "enum": [
        "available",
        "pending",
        "sold"
      ],
      "nullable": true,
      "x-nullable": true
    }
  },
  "required": [
    "id",
    "name",
    "photoUrls"
  ]
}